        }
    ],
    "require": {
        "anomaly/streams-composer-plugin": "~1.1",
        "anomaly/streams-platform": "1.1",
        "anomaly/default_authenticator-extension": "dev-develop",
        "anomaly/throttle_security_check-extension": "dev-develop",
        "anomaly/local_storage_adapter-extension": "dev-develop",
        "anomaly/default_page_handler-extension": "dev-develop",
        "anomaly/user_security_check-extension": "dev-develop",
        "anomaly/xml_feed_widget-extension": "dev-develop",
        "anomaly/page_link_type-extension": "dev-develop",
        "anomaly/url_link_type-extension": "dev-develop",
        "anomaly/relationship-field_type": "dev-develop",
        "anomaly/colorpicker-field_type": "dev-develop",
        "anomaly/polymorphic-field_type": "dev-develop",
        "anomaly/checkboxes-field_type": "dev-develop",
        "anomaly/encrypted-field_type": "dev-develop",
        "anomaly/datetime-field_type": "dev-develop",
        "anomaly/language-field_type": "dev-develop",
        "anomaly/multiple-field_type": "dev-develop",
        "anomaly/textarea-field_type": "dev-develop",
        "anomaly/markdown-field_type": "dev-develop",
        "anomaly/wysiwyg-field_type": "dev-develop",
        "anomaly/boolean-field_type": "dev-develop",
        "anomaly/country-field_type": "dev-develop",
        "anomaly/decimal-field_type": "dev-develop",
        "anomaly/integer-field_type": "dev-develop",
        "anomaly/editor-field_type": "dev-develop",
        "anomaly/select-field_type": "dev-develop",
        "anomaly/slider-field_type": "dev-develop",
        "anomaly/addon-field_type": "dev-master",
        "anomaly/email-field_type": "dev-develop",
        "anomaly/state-field_type": "dev-develop",
        "anomaly/files-field_type": "dev-develop",
        "anomaly/tags-field_type": "dev-develop",
        "anomaly/slug-field_type": "dev-develop",
        "anomaly/text-field_type": "dev-develop",
        "anomaly/file-field_type": "dev-develop",
        "anomaly/url-field_type": "dev-develop",
        "anomaly/configuration-module": "dev-develop",
        "anomaly/preferences-module": "dev-develop",
        "anomaly/navigation-module": "dev-develop",
        "anomaly/dashboard-module": "dev-develop",
        "anomaly/redirects-module": "dev-develop",
        "anomaly/installer-module": "dev-develop",
        "anomaly/variables-module": "dev-develop",
        "anomaly/settings-module": "dev-develop",
        "anomaly/addons-module": "dev-develop",
        "anomaly/users-module": "dev-develop",
        "anomaly/pages-module": "dev-develop",
        "anomaly/posts-module": "dev-develop",
        "anomaly/files-module": "dev-develop",
        "anomaly/contact-plugin": "dev-develop",
        "anomaly/helper-plugin": "dev-develop",
        "anomaly/robots-extension": "dev-develop",
        "anomaly/sitemap-extension": "dev-develop",
        "pyrocms/starter-theme": "dev-master",
        "pyrocms/pyrocms-theme": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "psr-0": {
            "": "tmp/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {},
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "3.1/master": "3.1.x-dev"
         }
    },
    "minimum-stability": "dev"
}
