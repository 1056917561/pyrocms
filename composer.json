{
    "name": "anomaly/streams",
    "description": "A powerful platform for deploying modular applications.",
    "keywords": ["cms", "streams", "laravel"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "hello@anomaly.is"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:anomalylabs/framework.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:anomalylabs/html.git"
        }
    ],
    "require": {
        "laravel/framework": "dev-master#cc1c35069a7bbc3717487d931fbd80b8e6641a90 as 5.0",
        "illuminate/html": "dev-master#b04be9a84be0d6f7552b281ad6b9e2795218fe56 as 5.0",
        "anomaly/streams-platform": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "source",
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
