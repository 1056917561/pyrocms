{
    "name": "anomaly/streams",
    "description": "A powerful platform for deploying modular applications.",
    "keywords": ["cms", "streams", "laravel"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "AnomalyLabs",
            "email": "hello@anomaly.is"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:anomalylabs/framework.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:anomalylabs/phpspec-laravel.git"
        }
    ],
    "require": {
        "anomaly/streams-platform": "~1.0"
    },
    "require-dev": {
        "anomaly/users_module_blocked_check-extension": "~1.0",
        "anomaly/users_module_default_authenticator-extension": "~1.0",
        "anomaly/users_module_activation_check-extension": "~1.0",
        "anomaly/streams_ckeditor_integration-extension": "~1.0",
        "anomaly/streams_ace_integration-extension": "~1.0",
        "anomaly/streams-composer-plugin": "~0.1",
        "anomaly/streams-distribution": "~1.0",
        "anomaly/boolean-field-type": "~1.0",
        "anomaly/checkboxes-field-type": "~1.0",
        "anomaly/country-field-type": "~1.0",
        "anomaly/datetime-field-type": "~1.0",
        "anomaly/editor-field-type": "~1.0",
        "anomaly/email-field-type": "~1.0",
        "anomaly/encrypted-field-type": "~1.0",
        "anomaly/file-field-type": "~1.0",
        "anomaly/integer-field-type": "~1.0",
        "anomaly/keywords-field-type": "~1.0",
        "anomaly/language-field-type": "~1.0",
        "anomaly/multiple-field-type": "~1.0",
        "anomaly/polymorphic-field-type": "~1.0",
        "anomaly/relationship-field-type": "~1.0",
        "anomaly/select-field-type": "~1.0",
        "anomaly/slider-field-type": "~1.0",
        "anomaly/slug-field-type": "~1.0",
        "anomaly/state-field-type": "~1.0",
        "anomaly/text-field-type": "~1.0",
        "anomaly/textarea-field-type": "~1.0",
        "anomaly/url-field-type": "~1.0",
        "anomaly/wysiwyg-field-type": "~1.0",
        "anomaly/addons-module": "~1.0",
        "anomaly/dashboard-module": "~1.0",
        "anomaly/preferences-module": "~1.0",
        "anomaly/settings-module": "~1.0",
        "anomaly/users-module": "~1.0",
        "anomaly/asset-tag": "~1.0",
        "anomaly/config-tag": "~1.0",
        "anomaly/datetime-tag": "~1.0",
        "anomaly/form-tag": "~1.0",
        "anomaly/helper-tag": "~1.0",
        "anomaly/html-tag": "~1.0",
        "anomaly/image-tag": "~1.0",
        "anomaly/input-tag": "~1.0",
        "anomaly/lang-tag": "~1.0",
        "anomaly/messages-tag": "~1.0",
        "anomaly/module-tag": "~1.0",
        "anomaly/request-tag": "~1.0",
        "anomaly/string-tag": "~1.0",
        "anomaly/theme-tag": "~1.0",
        "anomaly/url-tag": "~1.0",
        "anomaly/user-tag": "~1.0",
        "anomaly/streams-theme": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "source",
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
