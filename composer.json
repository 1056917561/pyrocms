{
	"name": "streams/streams",
	"description": "Streams Platform.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
        "laravel/framework": "4.3.*",
        "streams/core": "dev-master"
	},
    "require-dev": {
        "barryvdh/laravel-ide-helper": "1.*",
        "phing/phing": "2.*",
        "phpunit/phpunit": "4.*"
    },
	"autoload": {
		"classmap": [
			"database",
			"tests/TestCase.php"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
